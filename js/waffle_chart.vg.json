{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json", 
    "title": {
        "text": "Vaccine Brands used from 1st June 2021 to 1st September 2021",
        "font": "sans-serif",
        "fontSize": 17
    },
    "width": 400,
    "height": 400,
    "data": {"url": "https://raw.githubusercontent.com/johanazlan/Data-Visualization-Assignment-2/main/data/waffle_chart.csv"},
    "mark": {"type": "circle", "size": 1200}, 
    "encoding": {
        "x": {
            "field": "column",
            "type": "quantitative", 
            "axis": null
        }, 
        "y": {
            "field": "row", 
            "type": "quantitative",
            "axis": null
        }, 
        "color": {
            "field": "vax_type",
            "type": "nominal",
            "scale": {
                "type": "ordinal",
                "domain": ["full", "partial", "unvaccinated"],
                "range": ["#31a354", "#bae4b3","#ECECEC"]
            }
        }
        
    }, 
    "config": {
        "style": {
          "cell": {
            "stroke": "transparent"
          }
        }
      }

}